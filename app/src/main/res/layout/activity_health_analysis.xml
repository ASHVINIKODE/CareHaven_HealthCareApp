<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".HealthAnalysis">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:padding="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/healthAnalyticsLinearLayout">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Health Analytics Dashboard"
                android:textColor="@android:color/black"
                android:textSize="24sp"
                android:textStyle="bold"
                android:paddingBottom="16dp"
                android:gravity="center"/>

            <Button
                android:id="@+id/btnHeartRateBP"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Heart Rate/BP Trends"
                android:backgroundTint="@android:color/black"
                android:textColor="@android:color/white"
                android:padding="16dp"
                android:layout_marginTop="8dp"
                android:onClick="openHeartBP"/>

            <Button
                android:id="@+id/btnWeightBMI"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Weight &amp; BMI Changes"
                android:backgroundTint="@android:color/black"
                android:textColor="@android:color/white"
                android:padding="16dp"
                android:layout_marginTop="8dp"
                android:onClick="openWeightBMI"/>

            <Button
                android:id="@+id/btnBloodSugarLevels"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Blood Sugar Levels"
                android:backgroundTint="@android:color/black"
                android:textColor="@android:color/white"
                android:padding="16dp"
                android:layout_marginTop="8dp"
                android:onClick="openBloodSugarLevels"/>

            <Button
                android:id="@+id/btnDoctorInsights"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Doctor Insights"
                android:backgroundTint="@android:color/black"
                android:textColor="@android:color/white"
                android:padding="16dp"
                android:layout_marginTop="8dp"
                android:onClick="openDoctorInsight"/>

            <Button
                android:id="@+id/btnOverallHealthSummary"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Overall Health Summary"
                android:backgroundTint="@android:color/black"
                android:textColor="@android:color/white"
                android:padding="16dp"
                android:layout_marginTop="8dp"
                android:onClick="openOverallHealthSummary"/>

<!--            <Button-->
<!--                android:id="@+id/buttonNext"-->
<!--                android:layout_width="120dp"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:layout_gravity="end"-->
<!--                android:layout_marginBottom="30dp"-->
<!--                android:backgroundTint="#070707"-->
<!--                android:padding="4dp"-->
<!--                android:text="Back"-->
<!--                android:textColor="@android:color/white"-->
<!--                android:textSize="18sp" />-->

        </LinearLayout>

    </ScrollView>

</RelativeLayout>